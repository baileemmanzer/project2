<div class="row">
    <div class="col-md-6">
        <div data-id="{{id}}">
            <h4>Ingredient: {{ingredient}}</h4>
            <p class="expires-text">Expires: <span>{{expirationDate}}</span></p>
            <button data-id="{{id}}" data-ingredient="{{ingredient}}" data-quantity="{{quantity}}" class="exp-shop btn">Move Item to Shopping List</button>
            <hr>
        </div>
    </div>
</div>